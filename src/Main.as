package{	import com.rindra.events.VPEvent;	import com.rindra.utils.VP;	import flash.display.Sprite;	public class Main extends Sprite{		private var vpReady:Boolean = false;		private var vp:VP;		private var disableAutoRestartVideo:Boolean = false;				public function Main(){			vp = new VP('350x197', 'rtmp://cp33413.edgefcs.net/ondemand/cs/mp4:2013/gillette/gillette_3_125k_300x168_v2.mp4', true, false, true, true, true);			vp.addEventListener(VPEvent.VIDEO_START, _onStart);			addChild(vp);			vp.help(); //help for vp constructor		}				private function _onStart($e:VPEvent):void{			if(!vpReady){				vpReady = true;				vp.addEventListener(VPEvent.VIDEO_COMPLETE, _onComplete);				vp.addEventListener(VPEvent.VIDEO_END, _onEnd);				vp.addEventListener(VPEvent.VIDEO_PAUSE, _onPause);				vp.addEventListener(VPEvent.VIDEO_10S, _on10s);				vp.addEventListener(VPEvent.VIDEO_MIDPOINT, _onMidPoint);				vp.addEventListener(VPEvent.VIDEO_REPLAY, _onReplay);				vp.addEventListener(VPEvent.VIDEO_MUTE, _onMute);				vp.addEventListener(VPEvent.VIDEO_UNMUTE, _onUnmute);			}			//vp.viewControllerOnStart(false); //auto hide controller		}		private function _onComplete($e:VPEvent):void{			trace('video complete'); //fires 10s before the end of the video		}		private function _onEnd($e:VPEvent):void{			trace('video end');			if(!disableAutoRestartVideo) vp.restartVideo();		}		private function _onPause($e:VPEvent):void{			trace('video pause');		}		private function _on10s($e:VPEvent):void{			trace('video 10s_mark');		}		private function _onMidPoint($e:VPEvent):void{			trace('video mid_mark');		}		private function _onReplay($e:VPEvent):void{			trace('video replay');		}		private function _onMute($e:VPEvent):void{			trace('video mute');		}		private function _onUnmute($e:VPEvent):void{			trace('video unmute');			vp.restartVideo();			disableAutoRestartVideo = true;		}	}}